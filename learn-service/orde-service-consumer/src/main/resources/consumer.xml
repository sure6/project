<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <context:component-scan base-package="com.uow.service.impl"/>
    <dubbo:application name="orde-service-consumer"/>
    <dubbo:registry protocol="zookeeper" address="192.168.56.104:2181"/>
<!--    声明需要调用的远程服务接口, 生成远程服务代理-->
    <dubbo:reference interface="com.uow.service.UserService" id="userService"/>
</beans>